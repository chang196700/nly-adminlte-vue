{
  "name": "@nly-adminlte-vue/button",
  "version": "1.0.0",
  "meta": {
    "title": "Button",
    "subtitle": "按钮",
    "description": "具有NlyAdminlteVue主题特征的按钮",
    "components": [
      {
        "component": "NlyButton",
        "aliases": [
          "NlyBtn"
        ],
        "props": [
          {
            "prop": "isNavbar",
            "description": "放置到navbar form input group 中的表单按钮"
          },
          {
            "prop": "size",
            "description": "按钮大小，可选lg，sm，sx"
          },
          {
            "prop": "type",
            "description": "按钮类型，可选button，submit"
          },
          {
            "prop": "shape",
            "description": "按钮形状，可选roundedFlat，roundedPill，roundedCircle，roundedLg，roundedSm"
          },
          {
            "prop": "buttonClass",
            "description": "自定义css式样"
          },
          {
            "prop": "tool",
            "description": "工具类按钮"
          },
          {
            "prop": "app",
            "description": "app类按钮"
          },
          {
            "prop": "pressed",
            "description": "使按钮具有激活状态"
          },
          {
            "prop": "variant",
            "description": "btn类颜色，可选 dark，light，default，primary，secondary，success，info，danger，warning，outlineDark，outlineLight，outlinePrimary，outlineSecondary，outlineSuccess，outlineInfo，outlineDanger，outlineWarning"
          },
          {
            "prop": "block",
            "description": "让按钮填充整个父元素"
          }
        ],
        "events": [
          {
            "event": "click",
            "description": "disabled为false时点击按钮触发事件",
            "args": [
              {
                "name": "event",
                "type": "鼠标事件",
                "description": "原生点击事件"
              }
            ]
          }
        ]
      },
      {
        "component": "NlyButtonClose",
        "aliases": [
          "NlyBtnClose"
        ],
        "props": [
          {
            "prop": "content",
            "description": "文本内容"
          }
        ],
        "events": [
          {
            "event": "click",
            "description": "disabled为false时点击按钮触发事件",
            "args": [
              {
                "name": "event",
                "type": "鼠标事件",
                "description": "原生点击事件"
              }
            ]
          }
        ]
      }
    ]
  }
}