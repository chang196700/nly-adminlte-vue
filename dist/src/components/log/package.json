{
  "name": "@nly-adminlte-vue/log",
  "version": "1.0.0",
  "meta": {
    "title": "Log",
    "subtitle": "日志",
    "description": "日志组件是一个用来读取日志展示的组件,支持实时刷新滚动,底部锁定,顶部锁定等功能",
    "components": [
      {
        "component": "NlyLog",
        "props": [
          {
            "prop": "logClass",
            "description": "log自定义css"
          },
          {
            "prop": "containerTag",
            "description": "容器tag"
          },
          {
            "prop": "containerClass",
            "description": "容器自定义css"
          }
        ]
      },
      {
        "component": "NlyLogHeader",
        "props": [
          {
            "prop": "logHeaderClass",
            "description": "header自定义css"
          },
          {
            "prop": "titleTag",
            "description": "title tag"
          },
          {
            "prop": "titleClass",
            "description": "title自定义css"
          },
          {
            "prop": "title",
            "description": "title内容"
          }
        ]
      },
      {
        "component": "NlyLogBody",
        "props": [
          {
            "prop": "scrollBottom",
            "description": "开启滚动到底部按钮"
          },
          {
            "prop": "bottomIcon",
            "description": "滚动到底部按钮图标"
          },
          {
            "prop": "bottomText",
            "description": "滚动到底部按钮文字"
          },
          {
            "prop": "scrollTop",
            "description": "开启滚动到顶部按钮"
          },
          {
            "prop": "topIcon",
            "description": "滚动到顶部按钮图标"
          },
          {
            "prop": "logBodyClass",
            "description": "log body 自定义css"
          },
          {
            "prop": "preClass",
            "description": "pre标签自定义css式样"
          },
          {
            "prop": "lockScrollBottom",
            "description": "开启锁定到底部按钮"
          },
          {
            "prop": "lockBottomIcon",
            "description": "锁定底部按钮图标"
          },
          {
            "prop": "lockBottomText",
            "description": "锁定到底部按钮文字"
          }
        ]
      },
      {
        "component": "NlyLogLine",
        "props": [
          {
            "prop": "duration",
            "description": "时间，传入会渲染time组件"
          },
          {
            "prop": "durationClass",
            "description": "time组件自定义css"
          },
          {
            "prop": "line",
            "description": "log行数"
          },
          {
            "prop": "lineClass",
            "description": "log行数自定义css"
          },
          {
            "prop": "text",
            "description": "log内容"
          },
          {
            "prop": "textClass",
            "description": "log内容自定义css"
          },
          {
            "prop": "title",
            "description": "log内容标题"
          },
          {
            "prop": "titleClass",
            "description": "log内容标题自定义css"
          },
          {
            "prop": "icon",
            "description": "log内容前面的icon图标"
          },
          {
            "prop": "iconClass",
            "description": "log内容icon自定义css"
          },
          {
            "prop": "logLineClass",
            "description": "log自定义css"
          },
          {
            "prop": "highLight",
            "description": "高亮"
          }
        ]
      },
      {
        "component": "NlyLogLineTree",
        "props": [
          {
            "prop": "duration",
            "description": "时间，传入会渲染time组件"
          },
          {
            "prop": "durationClass",
            "description": "time组件自定义css"
          },
          {
            "prop": "line",
            "description": "log行数"
          },
          {
            "prop": "lineClass",
            "description": "log行数自定义css"
          },
          {
            "prop": "text",
            "description": "log内容"
          },
          {
            "prop": "textClass",
            "description": "log内容自定义css"
          },
          {
            "prop": "title",
            "description": "log内容标题"
          },
          {
            "prop": "titleClass",
            "description": "log内容标题自定义css"
          },
          {
            "prop": "icon",
            "description": "log内容前面的icon图标"
          },
          {
            "prop": "iconClass",
            "description": "log内容icon自定义css"
          },
          {
            "prop": "logLineClass",
            "description": "log自定义css"
          },
          {
            "prop": "highLight",
            "description": "高亮"
          },
          {
            "prop": "visible",
            "description": "默认收起折叠后log，设置true会打开"
          }
        ]
      },
      {
        "component": "NlyLogTools",
        "props": [
          {
            "prop": "logToolsClass",
            "description": "自定义css"
          }
        ]
      }
    ]
  }
}
