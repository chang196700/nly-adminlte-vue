{
  "name": "@nly-adminlte-vue/progress",
  "version": "1.0.0",
  "meta": {
    "title": "Progress",
    "subtitle": "进度条",
    "description": "一个支持水平和垂直布局，支持显示数字，百分数，描述的进度条",
    "components": [
      {
        "component": "NlyProgress",
        "props": [
          {
            "prop": "variant",
            "description": "颜色，primary，secondary，info，success，danger，indigo，purple，pink，navy，light，warning，lightblue，olive，lime，fuchsia，maroon，blue，orange，teal，white，gray，dark"
          },
          {
            "prop": "vertical",
            "description": "垂直进度条，注意设置为true时，nly-progress组件下只能有一个nly-progress-bar组件"
          },
          {
            "prop": "size",
            "description": "大小，可选sm，xs，xxs。在vertical=true时，设置label-value或者label-value-percent的时候，会导致文字无法显示齐全。"
          },
          {
            "prop": "height",
            "description": "水平进度条的高度"
          },
          {
            "prop": "striped",
            "description": "条纹背景，默认无条纹"
          },
          {
            "prop": "animated",
            "description": "在striped=true的情况下，animated=true会使条纹背景有动画"
          },
          {
            "prop": "precision",
            "description": "设置label-value和label-value-percent的小数点位数"
          },
          {
            "prop": "labelValuePercent",
            "description": "设置为true会以百分数展示当前进度值，会覆盖label-value"
          },
          {
            "prop": "labelValue",
            "description": "设置为true会展示当前进度值"
          },
          {
            "prop": "max",
            "description": "总进度值"
          },
          {
            "prop": "value",
            "description": "当前进度值"
          },
          {
            "prop": "progressClass",
            "description": "progress自定义css式样"
          },
          {
            "prop": "progressBarClass",
            "description": "progress-bar自定义css式样"
          }
        ]
      },
      {
        "component": "NlyProgressBar",
        "props": [
          {
            "prop": "variant",
            "description": "颜色，primary，secondary，info，success，danger，indigo，purple，pink，navy，light，warning，lightblue，olive，lime，fuchsia，maroon，blue，orange，teal，white，gray，dark"
          },
          {
            "prop": "label",
            "description": "label 标签文本"
          },
          {
            "prop": "labelHtml",
            "description": "label 标签的 html 字符串文本"
          },
          {
            "prop": "size",
            "description": "大小，可选sm，xs，xxs。在vertical=true时，设置label-value或者label-value-percent的时候，会导致文字无法显示齐全。"
          },
          {
            "prop": "striped",
            "description": "条纹背景，默认无条纹"
          },
          {
            "prop": "animated",
            "description": "在striped=true的情况下，animated=true会使条纹背景有动画"
          },
          {
            "prop": "precision",
            "description": "设置label-value和label-value-percent的小数点位数"
          },
          {
            "prop": "labelValuePercent",
            "description": "设置为true会以百分数展示当前进度值，会覆盖label-value"
          },
          {
            "prop": "labelValue",
            "description": "设置为true会展示当前进度值"
          },
          {
            "prop": "max",
            "description": "总进度值"
          },
          {
            "prop": "progressClass",
            "description": "progress自定义css式样"
          },
          {
            "prop": "progressBarClass",
            "description": "progress-bar自定义css式样"
          }
        ]
      }
    ]
  }
}
